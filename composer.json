{
    "name": "gopay-japan/gopay-sdk",
    "description": "PHP SDK for Gyro-n Gopay payments",
    "version": "1.0.0",
    "type": "library",
    "keywords": [],
    "homepage": "https://github.com/gyro-n/gopay-php-sdk",
    "license": "MIT",
    "require": {
        "php": ">=5.6.0",
        "rmccue/requests": "^1.7",
        "moneyphp/money": "^3.0"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.2.7",
        "squizlabs/php_codesniffer": "^3.2.0",
        "psy/psysh": "^0.9.4" 
    },
    "config": {
        "platform": {
            "php": "5.6.36"
        }
    },
    "autoload": {
        "psr-0": {
            "Gopay": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/Gopay"
        ]
    }
}
